{
	"name": "api-tmp",
	"version": "1.0.0",
	"description": "",
	"license": "ISC",
	"author": "",
	"type": "module",
	"main": "dist/index.js",
	"scripts": {
		"dev": "nodemon --exec tsx --env-file=.env.dev src/index.ts",
		"prebuild": "prisma generate",
		"build": "tsup",
		"start": "node --env-file=.env.prod dist/index.js",
		"start:staging": "node --env-file=.env.stage dist/index.js",
		"start:dev": "node --env-file=.env.dev dist/index.js",
		"swagger": "tsx src/config/swagger.ts",
		"prisma:generate": "dotenv -e .env.dev -- prisma generate",
		"prisma:generate:stage": "dotenv -e .env.stage -- prisma generate",
		"prisma:generate:prod": "dotenv -e .env.prod -- prisma generate",
		"prisma:migrate": "dotenv -e .env.dev -- prisma migrate dev",
		"prisma:migrate:stage": "dotenv -e .env.stage -- prisma migrate deploy",
		"prisma:migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
		"prisma:studio": "dotenv -e .env.dev -- prisma studio",
		"prisma:studio:stage": "dotenv -e .env.stage -- prisma studio",
		"prisma:studio:prod": "dotenv -e .env.prod -- prisma studio",
		"prisma:push": "dotenv -e .env.dev -- prisma db push",
		"prisma:push:stage": "dotenv -e .env.stage -- prisma db push",
		"prisma:push:prod": "dotenv -e .env.prod -- prisma db push",
		"prisma:reset": "dotenv -e .env.dev -- prisma migrate reset",
		"prisma:seed": "dotenv -e .env.dev -- prisma db seed"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@types/cookie-parser": "^1.4.10",
		"@types/cors": "^2.8.19",
		"@types/express": "^5.0.5",
		"@types/pg": "^8.15.6",
		"@types/swagger-ui-express": "^4.1.8",
		"dotenv": "^17.2.3",
		"dotenv-cli": "^8.0.0",
		"nodemon": "^3.1.11",
		"prisma": "^7.0.0",
		"swagger-autogen": "^2.23.7",
		"tsup": "^8.5.1",
		"tsx": "^4.20.6",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@prisma/adapter-pg": "^7.1.0",
		"@prisma/client": "^7.0.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"cross-env": "^10.1.0",
		"date-fns": "^4.1.0",
		"envalid": "^8.1.1",
		"express": "^5.1.0",
		"express-rate-limit": "^8.2.1",
		"helmet": "^8.1.0",
		"pg": "^8.16.3",
		"swagger-ui-express": "^5.0.1",
		"winston": "^3.18.3",
		"zod": "^4.1.13"
	}
}
